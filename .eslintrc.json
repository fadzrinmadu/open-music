{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "arrow-body-style": "off",
        "camelcase": "off",
        "newline-per-chained-call": "off",
        "no-console": "off",
        "no-underscore-dangle": "off",
        "object-curly-newline": "off"
    }
}
